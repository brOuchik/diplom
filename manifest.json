{
    "manifest_version": 3,
    "name": "AdTrafficBlocker",
    "version": "0.1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "deafult_title": "AdTrafficBlocker"
    },
    "permissions": ["tabs", "webNavigation", "scripting", "contextMenus"],
    "web_accessible_resources": [{
      "resources": ["Input.txt"],
      "matches": ["<all_urls>"]
    }]
  }